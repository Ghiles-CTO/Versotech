{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/cron/auto-match-reconciliation",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/cleanup-stale-locks",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/data-room-expiry",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/data-room-expiry-warnings",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/fees/generate-scheduled",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/publish-documents",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/unpublish-documents",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/kyc-expiry-reminders",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/term-sheet-reminders",
      "schedule": "0 9 * * *"
    }
  ]
}


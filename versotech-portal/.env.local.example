# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# n8n Webhook Configuration
# Invoice Generation System
N8N_INVOICE_GENERATION_WEBHOOK_URL=https://your-n8n-instance.com/webhook/invoice-generation
N8N_OUTBOUND_SECRET=your-outbound-webhook-secret
N8N_INBOUND_SECRET=your-inbound-webhook-secret

# E-Signature Integration (DocuSign/Dropbox Sign)
ESIGN_API_KEY=your-esign-api-key
ESIGN_WEBHOOK_SECRET=your-esign-webhook-secret

# Storage Configuration
STORAGE_BUCKET_NAME=documents
DEAL_DOCUMENTS_BUCKET=deal-documents
DOCS_BUCKET=docs

# Application Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Compliance AI Assistant (chat in flagged compliance threads)
# `local_codex` / `local_claude` use your local CLI subscription login for development.
# Switch to `api_openai` or `api_anthropic` for production.
COMPLIANCE_AI_PROVIDER=local_codex
COMPLIANCE_AI_CODEX_COMMAND=codex
COMPLIANCE_AI_CLAUDE_COMMAND=claude
# COMPLIANCE_AI_OPENAI_MODEL=gpt-4.1-mini
# COMPLIANCE_AI_ANTHROPIC_MODEL=claude-sonnet-4-5
# OPENAI_API_KEY=...
# ANTHROPIC_API_KEY=...

# KYC expiry OCR assistant (optional override; defaults to COMPLIANCE_AI_PROVIDER)
# KYC_EXPIRY_AI_PROVIDER=local_codex
# KYC_EXPIRY_AI_OPENAI_MODEL=gpt-4.1-mini
# KYC_EXPIRY_AI_CODEX_COMMAND=codex
# KYC_EXPIRY_AI_CLAUDE_COMMAND=claude
# KYC_EXPIRY_AI_DOCUMENT_TYPES=passport,national_id,id_card,residence_permit,visa,driving_license,proof_of_address

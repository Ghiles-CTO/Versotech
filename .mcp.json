{
  "mcpServers": {
    "supabase-old": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref=ipguxdssecfexudnvtia"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      }
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref=kagzryotbbnusdcyvqei"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      }
    },
    "executeautomation-playwright-server": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "linear": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-server-linear"
      ],
      "env": {
        "LINEAR_ACCESS_TOKEN": "${LINEAR_ACCESS_TOKEN}"
      }
    }
  }
